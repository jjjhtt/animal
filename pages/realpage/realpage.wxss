.realpagetext {
    margin: 10px;
    line-height: 25px;
    padding-bottom: 5px;
    border-bottom: 1px solid rgba(128, 128, 128, 0.8);
}
.dots {
    z-index: 999;
    position: absolute;
    right: 30rpx;
    top:200rpx;
    width: 70rpx;
    height: 35rpx;
    line-height: 35rpx;
    text-align: center;
    font-size: 24rpx;
    border-radius: 20rpx;
    background-color: rgba(0, 0, 0, 0.4);
    color: #fff;
  }
  .realpage_tag {
      margin: 10px 10px 0px 0px;
      float: left;
      color: skyblue;
  }
  .realpage_data {
      clear: both;
      font-size: 12px;
      color: rgba(0, 0, 0, 0.5);
  }
  .bottom-input {
    flex: 1;
    margin-left: 10px;
    padding-left: 10px;
    padding-right: 10px;
  }
  .realpage_comment {
    margin: 10px;
    padding-bottom: 50px;
  }
  .realpage_footer {
    position: fixed;
    display: flex;
    flex-direction: row;
    z-index: 99999;
    bottom: 0;
    height: 70rpx;
    border-top: solid 1px #ebebeb;
    background: #ffffff;
    width: 100%;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    /*padding-bottom: env(safe-area-inset-bottom);*/
  }
  .realPageIcon {
    margin-top: 1px;
    font-size:large;
  }