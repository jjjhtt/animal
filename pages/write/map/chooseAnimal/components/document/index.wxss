.document {
  display: flex;
}

.document__right {
  height: 100%;
  flex: auto;
  width: 0;
  position: relative;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
  background-color: white;
}

.flex {
  display: flex;
}

.document-normal-item-container {
  background-color: #fff;
  border-radius: 8rpx;
  padding-top: 28rpx;
  margin-top: -24rpx;
  margin-bottom: 30rpx;
  display: flex;
  flex-wrap: wrap;
}

.document-normal-item-container-item {
  height: 196rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 24rpx;
  width: 33.3%;
}

.document-normal-item-container-item .image {
  width: 144rpx;
  height: 144rpx;
}

.document-normal-item-container-item-title {
  text-align: center;
  width: 144rpx;
  font-size: 28rpx;
  color: #666666;
  margin-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: block;
  white-space:nowrap;
}
